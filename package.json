{
	"name": "typescriptStarter",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"bundle:ios:dev": "react-native bundle --platform=ios --dev=true --entry-file=index.ios.js --bundle-output ios/main.jsbundle --sourcemap-output ios/main.jsbundle.map",
		"bundle:ios:release": "react-native bundle --platform=ios --dev=false --entry-file=index.ios.js --bundle-output ios/main.jsbundle --sourcemap-output ios/main.jsbundle.map",
		"bundle:android:dev": "mkdir -p android/app/src/main/assets && react-native bundle --platform=android --dev=true --entry-file=index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/main.jsbundle.map",
		"bundle:android:release": "mkdir -p android/app/src/main/assets && react-native bundle --platform=android --dev=false --entry-file=index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/main.jsbundle.map",
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"test": "jest"
	},
	"dependencies": {
		"@types/react": "^16.0.40",
		"@types/react-native": "^0.52.16",
		"@types/react-redux": "^5.0.15",
		"react": "^16.3.0-alpha.1",
		"react-native": "0.54.0",
		"react-redux": "^5.0.7",
		"redux": "^3.6.0"
	},
	"devDependencies": {
		"babel-jest": "22.4.1",
		"babel-preset-react-native": "4.0.0",
		"jest": "22.4.2",
		"react-native-typescript-transformer": "^1.2.3",
		"react-test-renderer": "^16.3.0-alpha.1",
		"tslint": "^5.9.1",
        "tslint-react": "^3.5.1",
        "typescript": "^2.7.2"
	},
	"jest": {
		"preset": "react-native"
	}
}
